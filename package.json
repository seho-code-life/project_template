{
  "version": "0.0.0",
  "name": "project-template",
  "scripts": {
    "dev": "vite",
    "dev:test": "vite --mode test",
    "dev:prod": "vite --mode prod",
    "lint": "npm run lint:eslint && npm run lint:typescript",
    "lint:eslint": "eslint 'src/**/*' --fix",
    "lint:typescript": "tsc --noEmit --pretty false --skipLibCheck && vue-tsc --noEmit --skipLibCheck",
    "prettier": "prettier --write '**/*.{vue,jsx,tsx,js,ts,json,md}'",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "build": "npm run lint && vite build",
    "build:test": "npm run lint && vite build --mode test",
    "build:prod": "npm run lint && vite build --mode prod",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "serve": "vite preview"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "tsc --noEmit --pretty false --skipLibCheck",
      "git add"
    ],
    "*.{json,js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.vue": [
      "prettier --write",
      "eslint --fix",
      "vue-tsc --noEmit --skipLibCheck",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "@vueuse/core": "^6.4.1",
    "ant-design-vue": "^2.2.8",
    "axios": "^0.21.1",
    "dayjs": "^1.10.6",
    "kurimudb": "5.0.2",
    "kurimudb-driver-cookie": "5.0.1",
    "kurimudb-driver-localstorage": "5.0.1",
    "less": "^4.1.1",
    "mockjs": "^1.1.0",
    "pinia": "^2.0.0-rc.4",
    "query-string": "^7.0.1",
    "vue": "^3.2.12",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "@vitejs/plugin-legacy": "^1.5.1",
    "@vitejs/plugin-vue": "^1.4.0",
    "@vitejs/plugin-vue-jsx": "^1.1.7",
    "@vue/compiler-sfc": "^3.2.12",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.18.0",
    "husky": "^7.0.1",
    "jest": "^27.3.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "sass": "^1.38.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.3.5",
    "unplugin-auto-import": "^0.4.13",
    "unplugin-vue-components": "^0.16.0",
    "vite": "^2.5.0",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-pages": "^0.18.1",
    "vite-plugin-style-import": "^1.2.1",
    "vite-plugin-vue-layouts": "^0.4.1",
    "vite-plugin-windicss": "^1.4.12",
    "vue-tsc": "^0.28.10",
    "windicss": "^3.1.9"
  }
}
